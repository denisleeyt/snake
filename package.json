{
  "name": "@bybit-fe/snake",
  "description": "A configurable render library",
  "version": "0.1.8",
  "license": "MIT",
  "keywords": [
    "snake",
    "configurable",
    "low code"
  ],
  "scripts": {
    "start": "FORK_TS_CHECKER=1 dumi dev",
    "docs:build": "dumi build",
    "docs:zip": "dumi build && zip -rj snake-docs.zip docs-dist",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "build-watch": "father-build --watch",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:style": "stylelint \"src/**/*.less\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "release": "standard-version --release",
    "publish:dev": "node ./publish-dev.js",
    "prepublishOnly": "npm run build"
  },
  "main": "index.js",
  "typings": "index.d.ts",
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.23.2",
    "classnames": "^2.3.0",
    "lodash": "^4.17.0",
    "moment": "^2.29.0",
    "moment-timezone": "^0.5.34",
    "omit.js": "^2.0.2",
    "react": "^16.12.0 || ^17.0.0",
    "react-dom": "^16.12.0 || ^17.0.0",
    "react-router": "^5.2.0",
    "tapable": "^2.2.1",
    "umi-request": "^1.4.0"
  },
  "resolutions": {
    "@types/react": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.18",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.1.0",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "monaco-editor": "^0.33.0",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "prettier": "^2.2.1",
    "react-monaco-editor": "^0.48.0",
    "standard-version": "^9.3.2",
    "yorkie": "^2.0.0"
  }
}
